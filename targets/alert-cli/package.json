{
  "name": "alert-cli",
  "version": "1.13.1",
  "dependencies": {
    "@shared/graphql-client": "^1.13.1",
    "@socialgouv/cdtn-slugify": "^4.52.1",
    "@socialgouv/cdtn-sources": "^4.52.1",
    "@socialgouv/dila-api-client": "^1.2.1",
    "memoizee": "^0.4.15",
    "node-fetch": "^2.6.5",
    "nodegit": "0.27.0",
    "p-map": "4",
    "semver": "^7.3.5",
    "unist-util-parents": "^1.0.3",
    "unist-util-select": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@shared/dila-resolver": "^1.13.1",
    "@shared/types": "^1.13.1",
    "@socialgouv/contributions-data-types": "^3.27.0",
    "@socialgouv/eslint-config-typescript": "^1.88.0",
    "@socialgouv/fiches-travail-data-types": "^4.191.0",
    "@socialgouv/kali-data-types": "^2.127.0",
    "@socialgouv/legi-data-types": "^2.73.1",
    "@types/jest": "^27.0.2",
    "@types/memoizee": "^0.4.6",
    "@types/node": "^16.11.11",
    "@types/node-fetch": "^2.5.12",
    "@types/nodegit": "^0.27.3",
    "@types/semver": "^7.3.8",
    "@types/unist": "^2.0.6",
    "@vercel/ncc": "0.30.0",
    "babel-jest": "^27.1.1",
    "builtin-modules": "^3.2.0",
    "eslint": "^7.32.0",
    "jest": "^27.1.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.3"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "ncc build ./src/index.ts -o ./dist -e nodegit -s",
    "start": "node --enable-source-maps --unhandled-rejections=strict ./dist/index.js",
    "start:debug": "rm -rf data/* && ncc build ./src/index.ts -o ./dist -e nodegit -s && HASURA_GRAPHQL_ENDPOINT=http://localhost:8080/v1/graphql HASURA_GRAPHQL_ADMIN_SECRET=admin1 node --enable-source-maps --unhandled-rejections=strict ./dist/index.js",
    "lint": "eslint src",
    "clean": "rm -rf data/*",
    "types": "tsc",
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ]
  },
  "lint-staged": {
    "*.{j,t}s": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  }
}
