{
  "name": "alert-cli",
  "version": "1.0.0",
  "dependencies": {
    "@shared/graphql-client": "1.0.0",
    "@socialgouv/contributions-data": "^2.2.0",
    "nodegit": "0.26.5",
    "semver": "^7.3.2",
    "unist-util-parents": "^1.0.3",
    "unist-util-select": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-commonjs": "^13.0.1",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^12.12.50",
    "@types/nodegit": "^0.26.6",
    "@types/semver": "^7.3.1",
    "@types/unist": "^2.0.3",
    "babel-jest": "^26.1.0",
    "builtin-modules": "^3.1.0",
    "esm": "^3.2.25",
    "jest": "^26.1.0",
    "rollup": "^2.18.1",
    "typescript": "^3.9.6"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "start": "node --enable-source-maps --unhandled-rejections=strict ./dist/index.js",
    "start:dev": "GRAPHQL_ENDPOINT=http://localhost:8080/v1/graphql HASURA_GRAPHQL_ADMIN_SECRET=admin1 node -r esm src/index.js",
    "start:dump": "GRAPHQL_ENDPOINT=http://localhost:8080/v1/graphql HASURA_GRAPHQL_ADMIN_SECRET=admin1 DUMP=true node -r esm src/index.js > data/dump.json",
    "types": "tsc",
    "test": "jest"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!**/data/**"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["/node_modules/", "/data/"]
  }
}
