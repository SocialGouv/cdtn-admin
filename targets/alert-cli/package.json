{
  "name": "alert-cli",
  "version": "1.0.0",
  "dependencies": {
    "@shared/graphql-client": "1.0.0",
    "@socialgouv/cdtn-slugify": "^4.28.0",
    "@socialgouv/cdtn-sources": "^4.28.0",
    "@socialgouv/contributions-data": "^2.12.0",
    "@socialgouv/datafiller-data": "^1.48.0",
    "@socialgouv/fiches-travail-data": "^3.168.0",
    "nodegit": "0.27.0",
    "semver": "^7.3.2",
    "unist-util-parents": "^1.0.3",
    "unist-util-select": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@socialgouv/eslint-config-recommended": "^1.21.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.27",
    "@types/nodegit": "^0.26.7",
    "@types/semver": "^7.3.3",
    "@types/unist": "^2.0.3",
    "babel-jest": "^26.2.2",
    "builtin-modules": "^3.1.0",
    "eslint": "^7.6.0",
    "esm": "^3.2.25",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "rollup": "^2.23.1",
    "typescript": "^3.9.7"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "start": "node --enable-source-maps --unhandled-rejections=strict ./dist/index.js",
    "start:dev": "GRAPHQL_ENDPOINT=http://localhost:8080/v1/graphql HASURA_GRAPHQL_ADMIN_SECRET=admin1 node -r esm src/index.js",
    "start:dump": "GRAPHQL_ENDPOINT=http://localhost:8080/v1/graphql HASURA_GRAPHQL_ADMIN_SECRET=admin1 DUMP=true node -r esm src/index.js > data/dump.json",
    "lint": "eslint src/**/*.js",
    "types": "tsc",
    "test": "jest"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!**/data/**"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/data/"
    ]
  },
  "lint-staged": {
    "*.{j,t}s": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  }
}
