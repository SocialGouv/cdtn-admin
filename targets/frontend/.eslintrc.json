{
  "extends": [
    "@socialgouv/eslint-config-recommended",
    "@socialgouv/eslint-config-react"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "."]
      }
    },
    "jest": {
      "version": 26
    }
  },
  "rules": {
    "jsx-a11y/anchor-is-valid": "warn",
    "react/prop-types": "warn",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
  }
}
