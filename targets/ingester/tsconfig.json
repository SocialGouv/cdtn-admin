{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "strict": true,
    "lib": ["es2019"],
    "baseUrl": "."
  },
  "include": ["src", "@types"]
}
