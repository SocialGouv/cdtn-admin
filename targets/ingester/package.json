    "@types/node": "^14.14.31",
    "@types/semver": "^7.3.4",
    "@types/tar-fs": "^2.0.0",
    "@types/unist": "^2.0.3",
    "@vercel/ncc": "0.27.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "esm": "^3.2.25",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "license": "MIT",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "ncc build ./src/cli.js -o ./dist -s",
    "cli:dev": "HASURA_GRAPHQL_ENDPOINT=http://localhost:8080/v1/graphql HASURA_GRAPHQL_ADMIN_SECRET=admin1 node -r esm src/cli.js",
    "cli:prod": "HASURA_GRAPHQL_ENDPOINT=http://localhost:8080/v1/graphql HASURA_GRAPHQL_ADMIN_SECRET=admin1 yarn start",
    "lint": "eslint src/**/*.js",
    "start": "node --enable-source-maps --unhandled-rejections=strict dist/index.js",
    "test": "jest --passWithNoTests",
    "types": "tsc"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/data/"
    ]
  },
  "lint-staged": {
    "*.{j,t}s": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  }
}
