    "lint": "npx eslint \"./src/**/*.{js,ts}\"",
    "precommit": "lint-staged",
    "start": "node --enable-source-maps --unhandled-rejections=strict dist/index.js",
    "test": "jest --passWithNoTests",
    "types": "tsc"
  },
  "lint-staged": {
    "*.{j,t}s": [
      "npx eslint --cache --fix",
      "jest --bail --findRelatedTests"
    ]
  }
